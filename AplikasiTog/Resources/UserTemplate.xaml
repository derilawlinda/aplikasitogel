<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:userViewModels="clr-namespace:Apel.ViewModels.Users"
                    xmlns:userViews="clr-namespace:Apel.Views.Users"
                    >
    <DataTemplate DataType="{x:Type userViewModels:UserSearchViewModel}">
        <userViews:UserSearch/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type userViewModels:UserAddEditViewModel}">
        <userViews:UserAddEdit/>
    </DataTemplate>


</ResourceDictionary>